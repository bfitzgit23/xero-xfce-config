<?xml version="1.0" encoding="UTF-8"?>
<actions>
<action>
	<icon>cs-startup-programs</icon>
	<name>Run...</name>
	<unique-id>1526630606202525-4</unique-id>
	<command>xfce4-terminal -e %f</command>
	<description></description>
	<patterns>*</patterns>
	<other-files/>
	<text-files/>
</action>
<action>
	<icon>terminal</icon>
	<name>Open Terminal Here</name>
	<unique-id>1572857227152703-1</unique-id>
	<command>/usr/bin/xfce4-terminal</command>
	<description>Open Terminal Here</description>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>utilities-terminal_su</icon>
	<name>Open Root Terminal Here</name>
	<unique-id>1584948245322558-2</unique-id>
	<command>gksu /usr/bin/xfce4-terminal</command>
	<description></description>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>folder</icon>
	<name>Open Folder as Root</name>
	<unique-id>1549136221831273-4</unique-id>
	<command>gksu thunar %f</command>
	<description>Open Thunar as Root...</description>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>gtk-orientation-portrait</icon>
	<name>Ownership to Root</name>
	<unique-id>1526631155250282-8</unique-id>
	<command>gnome-keyring-daemon -r -d;gksu &quot;chown -R root:root %F&quot;</command>
	<description>Change permissions to root</description>
	<patterns>*</patterns>
	<startup-notify/>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
<action>
	<icon>gtk-orientation-reverse-portrait</icon>
	<name>Ownership to User</name>
	<unique-id>1526631178674034-9</unique-id>
	<command>gnome-keyring-daemon -r -d;gksu &quot;chown -R $USER:$USER %F&quot;</command>
	<description>Change permissions to user</description>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
<action>
	<icon>run-build-install-root</icon>
	<name>Make Script Executable</name>
	<unique-id>1526670239648084-2</unique-id>
	<command>chmod +x %f</command>
	<description>Make a file executable</description>
	<patterns>*</patterns>
	<other-files/>
	<text-files/>
</action>
<action>
	<icon>preferences-system-search</icon>
	<name>Search Files and Folders</name>
	<unique-id>1570767526797778-1</unique-id>
	<command>catfish --path=%f</command>
	<description>Search for Files and Folders</description>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>imagemagick</icon>
	<name>Convert PNG to JPG</name>
	<unique-id>1510662686010963-1</unique-id>
	<command>convert %f -quality 90 `basename %f .png`.jpg</command>
	<description>Convert PNG to JPG</description>
	<patterns>*.png</patterns>
	<image-files/>
</action>
</actions>
